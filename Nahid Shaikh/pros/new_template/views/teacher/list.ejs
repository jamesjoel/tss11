<div class="container mt-4 min-height:700px">
    <div class="row">
        <div class="col-md-6 offset-md-2">
            <h4>Student's Data </h4>
        </div>
        <table class="table table-dark table-hover table-bordered mt-4">
            <tr>
                <th>S.No.</th>
                <th>Name</th>
                <th>Salary</th>
               <th>Address</th>
               <th>Contact</th>
               <th>City</th>
               <th>Gender</th>
            </tr>

            <% result.forEach((x, n)=>{ %>
                <tr>
                    <td><%= n+1 %></td>
                    <td><%= x.name %></td>
                   
                    <td><%= x.contact %></td>
                    <td><a href="/teacher/view_data/<%= x._id %>" class="btn btn-sm btn-info">View</a></td>
                    <td><a href="#" class="btn btn-sm btn-warning">Edit</a></td>
                    <td><a href="#" class="btn btn-sm btn-danger">Delete</a></td>
                    
                </tr>
            <% }) %>
        </table>
    </div>
</div>
</div>