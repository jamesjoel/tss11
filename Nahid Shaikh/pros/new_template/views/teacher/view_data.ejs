<div class="container">
  <div class="row">
    <div class="col-md-8 offset-md-2">
      <br />
      <br />
      <table class="table table-dark table-bordered table-striped table-hover">
        <tr>
          <th>S.No.</th>
          <th>Name</th>
          <th>Salary</th>
          <th>Address</th>
          <th>Contact</th>
          <th>City</th>
          <th>Gender</th>
        </tr>
        <tr>
          <% result.forEach((x, n)=>{ %>
            <tr>
                <td><%= n+1 %></td>
                <td><%= x.name %></td>
                <td><%= x.fee %></td>
                <td><%= x.class %></td>
                <td><%= x.address %></td>
                <td><%= x.gender %></td>
                <td><%= x.contact %></td>
            </tr>
        <% }) %>
          
        </tr>
      </table>
    </div>
  </div>
</div>
