<div class="container">
  <div class="row">
    <div class="col-md-8 offset-md-2">
      <br />
      <br />
      <table class="table table-dark table-bordered table-striped table-hover">
        <tr>
          <th>S.No.</th>
          <th>Name</th>
          <th>Salary</th>
          <th>Address</th>
          <th>Contact</th>
          <th>City</th>
          <th>Gender</th>
        </tr>
        <tr>
          <% result.forEach((x, n)=>{ %>
            <tr>
                <td><%= n+1 %></td>
                <td><%= x.name %></td>
                <td><%= x.contact %></td>
                <td><a href="/teacher/list" class="btn btn-sm btn-success">view</a></td>
                <td><a href="/teacher/list" class="btn btn-sm btn-info">view</a></td>
                <td><a href="/teacher/list" class="btn btn-sm btn-danger">view</a></td>
                <td><a href="/teacher/list" class="btn btn-sm btn-secondary">view</a></td>
            </tr>
        <% }) %>
          
        </tr>
      </table>
    </div>
  </div>
</div>
