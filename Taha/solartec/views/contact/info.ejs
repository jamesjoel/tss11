<div class="container mt-4" style="min-height: 700px;">
    <div class="row">
        <div class="col-md-10 offset-md-1">
            <h4>Contact_User_list</h4>
            <br />
            <table class="table table-dark table-bordered">
                <tr>
                    <td>S.no</td>
                    <td>Full Name</td>
                    <td>email</td>
                    <td>Subject</td>
                    <td>Message</td>
                </tr>
                <%result.forEach((x,n)=>{%>
                <tr>
                    <td><%= n+1 %></td>
                    <td><%= x.name %></td>
                    <td><%= x.email %></td>
                    <td><%= x.subject %></td>
                    <td><%= x.message %></td>
                    </tr>               
                <%});%>
            </table>
        </div>
    </div>
</div>