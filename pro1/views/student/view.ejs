<div class="container" style="min-height: 700px;">
    <div class="row">
        <div class="col-md-10 offset-md-1">
            <h4>List of All Student</h4>
            <table class="table table-dark table-hover table-bordered mt-4">
                <tr>
                    <th>S.No.</th>
                    <th>Name</th>
                    <th>Fee</th>
                    <th>Class</th>
                    <th>Address</th>
                    <th>Gender</th>
                    <th>Contact</th>
                </tr>

                <% result.forEach((x, n)=>{ %>
                    <tr>
                        <td><%= n+1 %></td>
                        <td><%= x.name %></td>
                        <td><%= x.fee %></td>
                        <td><%= x.class %></td>
                        <td><%= x.address %></td>
                        <td><%= x.gender %></td>
                        <td><%= x.contact %></td>
                    </tr>
                <% }) %>
            </table>
        </div>
    </div>
</div>